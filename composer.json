{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "algolia/algoliasearch-client-php": "^3.0.0",
    "cyrildewit/eloquent-viewable": "^6.0",
    "fakerphp/faker": "^1.14",
    "inertiajs/inertia-laravel": "^v0.5.0",
    "laravel/fortify": "^1.7",
    "laravel/framework": "^v9.0.0",
    "laravel/scout": "^v9.0.0",
    "laravel/tinker": "^2.0",
    "laravolt/avatar": "^4.0.0",
    "parsedown/laravel": "^1.2.1",
    "pusher/pusher-php-server": "^7.0.0",
    "sentry/sentry-laravel": "^2.6.0",
    "spatie/laravel-cookie-consent": "^3.2",
    "tightenco/ziggy": "^1.3"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.2",
    "barryvdh/laravel-ide-helper": "^2.0.0",
    "jetbrains/phpstorm-attributes": "^1.0",
    "laravel/sail": "^1.8",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^v6.0.0",
    "nunomaduro/larastan": "^2.0",
    "pestphp/pest": "^1.9",
    "phpunit/phpunit": "^9.0",
    "spatie/laravel-ignition": "^1.0",
    "worksome/envy": "^0.3.1"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "larastan": [
      "./vendor/bin/phpstan analyse"
    ]
  }
}
